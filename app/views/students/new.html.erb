<h1> Hi! </h1>
<%= form_for :student, url: students_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.label :height %><br>
    <%= f.text_area :height %>
  </p>
  <p>
    <%= f.label :colour %><br>
    <%= f.text_area :colour %>
  </p>
  <p>
    <%= f.label :gpa %><br>
    <%= f.text_area :gpa %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', students_path %>
